<mujoco model="simpleWoodTable">
  <asset>
    <!-- Define yellow wood material -->
    <material name="yellow_wood" rgba="0.9 0.7 0.3 1"/>
  </asset>
  
  <worldbody>
    
    <!-- Table structure -->
    <body name="simpleWoodTable" pos="1.3 0 0">
      <!-- Tabletop -->
      <geom type="box" size=".375 .6 .025" pos="0 0 0.535" material="yellow_wood"/>
      
      <!-- Support layer -->
      <geom type="box" size=" 0.26 0.59 0.05" pos="0 0 0.46" material="yellow_wood"/>
      
      <!-- Legs -->
      <geom type="box" size="0.025 0.025 0.205" pos="0.235 -0.565  0.205" material="yellow_wood"/>
      <geom type="box" size="0.025 0.025 0.205" pos="-0.235 -0.565  0.205" material="yellow_wood"/>
      <geom type="box" size="0.025 0.025 0.205" pos="0.235 0.565  0.205" material="yellow_wood"/>
      <geom type="box" size="0.025 0.025 0.205" pos="-0.235 0.565  0.205" material="yellow_wood"/>
      
      <!-- Reinforcement beams -->
      <geom type="box" size="0.02 0.565  0.02" pos="0.235 0 0.25" material="yellow_wood"/>
      <geom type="box" size="0.02 0.565 0.02" pos="-0.235 0 0.25" material="yellow_wood"/>
    </body>
    
    <!-- Overhead camera -->
    <camera name="overhead" pos="0 -1.5 1.5" mode="fixed" quat="0.707 0 0.707 0"/>
  </worldbody>
</mujoco>